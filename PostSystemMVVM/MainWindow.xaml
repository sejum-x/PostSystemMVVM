<Window x:Class="PostSystemMVVM.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PostSystemMVVM"
        xmlns:view="clr-namespace:PostSystemMVVM.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
    </Window.Resources>
    <TabControl>
        <TabItem Header="Order" Background="#fefffa">
            <view:OrderUserControl/>
        </TabItem>
        <TabItem Header="Branch" Background="#fefffa">
            <view:BranchUserControl/>
        </TabItem>
        <TabItem Header="Client" Background="#fefffa">
            <view:ClientUserControl/>
        </TabItem>
        <TabItem Header="Worker" Background="#fefffa">
            <view:WorkerUserControl/>
        </TabItem>
    </TabControl>
</Window>
